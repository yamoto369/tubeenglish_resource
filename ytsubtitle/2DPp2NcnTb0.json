  {
    "startTime": 284.8,
    "endTime": 286.16,
    "text": "america"
  },
  {
    "startTime": 286.16,
    "endTime": 289.12,
    "text": "as yet nobody has used clt to build"
  },
  {
    "startTime": 289.12,
    "endTime": 289.68,
    "text": "beyond"
  },
  {
    "startTime": 289.68,
    "endTime": 293.759,
    "text": "55 meters but michael ramage's research"
  },
  {
    "startTime": 293.759,
    "endTime": 295.199,
    "text": "center in cambridge"
  },
  {
    "startTime": 295.199,
    "endTime": 297.52,
    "text": "working with another london practice has"
  },
  {
    "startTime": 297.52,
    "endTime": 299.199,
    "text": "proposed a concept design"
  },
  {
    "startTime": 299.199,
    "endTime": 301.919,
    "text": "of a 300 meter tower to be built on top"
  },
  {
    "startTime": 301.919,
    "endTime": 304.08,
    "text": "of one of london's most iconic concrete"
  },
  {
    "startTime": 304.08,
    "endTime": 304.96,
    "text": "structures"
  },
  {
