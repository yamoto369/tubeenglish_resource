  },
  {
    "startTime": 960.399,
    "endTime": 962.72,
    "text": "and"
  },
  {
    "startTime": 964.639,
    "endTime": 967.279,
    "text": "we talked about what it's like"
  },
  {
    "startTime": 967.279,
    "endTime": 968.959,
    "text": "you know i said i too i grew up on the"
  },
  {
    "startTime": 968.959,
    "endTime": 971.12,
    "text": "rez i too have"
  },
  {
    "startTime": 971.12,
    "endTime": 973.04,
