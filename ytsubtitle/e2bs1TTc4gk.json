  {
    "startTime": 996.24,
    "endTime": 998.16,
    "text": "what would you like me"
  },
  {
    "startTime": 998.16,
    "endTime": 999.68,
    "text": "to tell the people"
  },
  {
    "startTime": 999.68,
    "endTime": 1001.279,
    "text": "and he said"
  },
  {
    "startTime": 1001.279,
    "endTime": 1003.279,
