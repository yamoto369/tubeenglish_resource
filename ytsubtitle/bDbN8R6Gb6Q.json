  },
  {
    "startTime": 58.079,
    "endTime": 59.52,
    "text": "until the day i was born"
  },
  {
    "startTime": 59.52,
    "endTime": 61.84,
    "text": "my parents were expecting a perfectly"
  },
  {
    "startTime": 61.84,
    "endTime": 63.52,
    "text": "normal baby girl"
  },
  {
    "startTime": 63.52,
    "endTime": 67.6,
    "text": "then i popped out"
  },
  {
    "startTime": 67.6,
    "endTime": 69.84,
    "text": "you see no one not even the doctors"
  },
  {
    "startTime": 69.84,
    "endTime": 72.0,
    "text": "realized that was one of the 1 in"
  },
  {
    "startTime": 72.0,
    "endTime": 75.04,
    "text": "40 000 babies born with fatter syndrome"
  },
  {
    "startTime": 75.04,
    "endTime": 76.4,
    "text": "every year"
  },
  {
