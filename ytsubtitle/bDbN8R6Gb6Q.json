  },
  {
    "startTime": 172.879,
    "endTime": 173.36,
    "text": "friends"
  },
  {
    "startTime": 173.36,
    "endTime": 177.68,
    "text": "and neither did anyone else"
  },
  {
    "startTime": 177.68,
    "endTime": 179.68,
    "text": "the way i saw it the only thing wrong"
  },
  {
    "startTime": 179.68,
    "endTime": 181.28,
