  },
  {
    "startTime": 181.28,
    "endTime": 183.36,
    "text": "something"
  },
  {
    "startTime": 183.36,
    "endTime": 186.319,
    "text": "a little over a year ago well okay let's"
  },
  {
    "startTime": 186.319,
    "endTime": 188.48,
    "text": "start at the beginning"
  },
  {
    "startTime": 188.48,
    "endTime": 190.48,
    "text": "ever since i was little i have been very"
  },
  {
    "startTime": 190.48,
    "endTime": 192.319,
    "text": "competitive"
  },
  {
    "startTime": 192.319,
