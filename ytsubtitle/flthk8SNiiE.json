  {
    "startTime": 176.239,
    "endTime": 177.84,
    "text": "yourself"
  },
  {
    "startTime": 177.84,
    "endTime": 180.159,
    "text": "do i really need that adverb for"
  },
  {
    "startTime": 180.159,
    "endTime": 182.48,
