  },
  {
    "startTime": 104.94,
    "endTime": 107.89999999999999,
    "text": "Should I do it really slowly?"
  },
  {
    "startTime": 107.1,
    "endTime": 108.39999999999999,
    "text": "Yeah, do it slowly."
  },
  {
    "startTime": 108.4,
    "endTime": 109.30000000000001,
    "text": "Yes."
  },
  {
    "startTime": 112.534,
    "endTime": 115.31,
    "text": "That's awesome."
  },
  {
    "startTime": 115.31,
    "endTime": 116.58,
    "text": "I like that."
  },
  {
    "startTime": 116.58,
    "endTime": 118.22,
    "text": "The classic purple shorts."
  },
  {
    "startTime": 118.22,
    "endTime": 119.5,
