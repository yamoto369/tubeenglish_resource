[
  {
    "startTime": 2.201,
    "endTime": 5.1370000000000005,
    "text": "Noah Sanders. Here for the job interview."
  },
  {
    "startTime": 5.137,
    "endTime": 7.0729999999999995,
    "text": "Please take a seat. Mr. Weinstein will see you next."
  },
  {
    "startTime": 7.073,
