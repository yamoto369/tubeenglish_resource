  },
  {
    "startTime": 51.922,
    "endTime": 55.599,
    "text": "(punching and grunting)"
  },
  {
    "startTime": 55.6,
    "endTime": 56.879000000000005,
    "text": "- This is a bad idea."
  },
  {
    "startTime": 56.879,
    "endTime": 59.711,
    "text": "- Is it ever a good one, honestly?"
  },
  {
    "startTime": 61.76,
    "endTime": 64.235,
    "text": "- He's not just some observer."
  },
  {
    "startTime": 64.235,
    "endTime": 65.962,
    "text": "He's an assassin."
  },
  {
    "startTime": 65.962,
    "endTime": 69.436,
    "text": "- Do not trust anybody outside of this room."
  },
  {
    "startTime": 69.436,
    "endTime": 70.95,
    "text": "- You go rogue, he's been authorized"
  },
  {
    "startTime": 70.95,
    "endTime": 72.629,
    "text": "to hunt you down and kill you."
  },
  {
    "startTime": 72.629,
    "endTime": 74.46000000000001,
    "text": "- That's the job."
  },
  {
    "startTime": 76.16,
    "endTime": 77.301,
    "text": "No hard feelings."
  },
  {
    "startTime": 77.301,
    "endTime": 80.276,
